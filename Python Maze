t.right(90)
t.forward(125)
t.speed(0)
t.shape(name="turtle")
t.setpos(0,0)

colors = ['red', 'orange', 'yellow', 'green', 'blue']
          
for i in range(5000):
	t.forward(i)
	t.color(colors[i % len(colors)])
	t.left(500)
